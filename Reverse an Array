import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
    Scanner s=new Scanner(System.in);
    int n,k;
    k=s.nextInt();
    int arr[]=new int[k];
    for(int i=0;i<k;i++){
        arr[i]=s.nextInt();
    }
    for(int i=k-1;i>=0;i--){
        System.out.println(arr[i]);
    }
    
    }
}
